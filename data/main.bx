const tests = [
    "generator",
    "array",
    "assign",
    "comments",
    "continue_break",
    "import",
    "fact",
    "for",
    "fun",
    "if",
    "indent",
    "is",
    "object",
    "string_fmt",
    "while"
]

const stars = "*" * 25

for (test in tests) {
    print(stars)
    print(test)
    print(stars)
    require("tests." + test)
    print(stars)
    print("")
}